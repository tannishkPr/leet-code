class Solution {
    public int maxVowels(String s, int k) {
       int[] map = new int[26];
        for(int i=0;i<k;i++){
            map[s.charAt(i)-'a']++;
        }
        int max= 0;
         int j = 0;
        for(int i=k;i<s.length();i++){
            map[s.charAt(i)-'a']++;
             map[s.charAt(j)-'a']--;
             j++;
            max = Math.max(max,count(map));
        }
        if(max<= k)
        return max;
        else
          Return k;
    }
     public int count(int[] map) {
         int count = 0;
         String ans = "aeiou";
            for(int i=0;i<ans.length();i++){
                count += map[ans.charAt(i)-'a'];
        }
         return count;
    }
    
}
